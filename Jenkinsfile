pipeline {
  agent any
  stages {
    stage('zsdfd') {
      parallel {
        stage('zsdfd') {
          steps {
            echo 'hello'
            sh 'echo "Hello"'
          }
        }
        stage('sdfd') {
          steps {
            echo 'fdgdfgdfgd'
          }
        }
      }
    }
    stage('zdfsdf') {
      parallel {
        stage('zdfsdf') {
          steps {
            sh 'echo "dfkdf"'
          }
        }
        stage('cvdfbv') {
          steps {
            echo 'mvkldfmvdklfmv'
          }
        }
      }
    }
  }
}